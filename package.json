{
  "name": "xxl-job-webpanel-r3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "vite --mode developer_local --port 80 --host",
    "dev": "vite --mode development --port 80 --host",
    "build:local": "tsc && vite build --mode developer_local",
    "build:dev": "tsc && vite build --mode development",
